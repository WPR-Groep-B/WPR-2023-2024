public class onderzoek {
    int onderzoekId;

    string onderzoekNaam;

    string onderzoekBeschrijving;

    gebruikerBedrijf gebruikerBedrijf;

    gebruikerDeskundige gebruikerDeskundige;


}

public class gebruiker {
    int gebruikerId;

    string voornaam;

    string achternaam;

    string email;

}

public class gebruikerBedrijf : gebruiker {

    string bedrijfsnaam;

}

public class gebruikerDeskundige : gebruiker {

    string beperking;

}